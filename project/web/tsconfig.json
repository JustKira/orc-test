{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"outDir": "./build",
		"moduleResolution": "node",
		"target": "es2020",
		"module": "esnext",
		"lib": ["dom", "es2020"],
		"allowJs": true,
		"checkJs": false,
		"jsx": "preserve",
		"sourceMap": true,
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"types": ["svelte"]
	},
	"include": ["src/**/*", "src/node_modules/@types"],
	"exclude": ["node_modules/*", "__sapper__/*", "static/*"]
}
